#Simple Password Generator
import random

alphabet = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890-!@#$%^&*'


def password_generator(lenght):
    final_password = ''
    counter = 0
    while cntr < lenght:
        final_password += random.choice(alphabet)
        cntr += 1
    return final_password


print(generator(10))
